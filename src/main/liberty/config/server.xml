<server description="Template Rest Application">
    <featureManager>
        <feature>jaxrs-2.1</feature>
        <feature>cdi-2.0</feature>
    </featureManager>
    <httpEndpoint httpPort="${default.http.port}" httpsPort="${default.https.port}" id="defaultHttpEndpoint" host="*" />
    <webApplication id="Template" location="Template.war" contextRoot="${app.context.root}" />
</server>